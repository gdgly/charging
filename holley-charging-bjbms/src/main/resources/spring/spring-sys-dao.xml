	<bean id="sysNoticeTemplateMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="com.holley.platform.dao.sys.SysNoticeTemplateMapper" />
	</bean>
	<bean id="sysOperationLogMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="com.holley.platform.dao.sys.SysOperationLogMapper" />
	</bean>	
	<bean id="sysRoleMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="com.holley.platform.dao.sys.SysRoleMapper" />
	</bean>
	<bean id="sysRolemoduleMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="com.holley.platform.dao.sys.SysRolemoduleMapper" />
	</bean>	
	<bean id="sysRuleMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="com.holley.platform.dao.sys.SysRuleMapper" />
	</bean>		
	<bean id="sysCarBrandMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="com.holley.platform.dao.sys.SysCarBrandMapper" />
	</bean>
</beans>